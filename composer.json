	"homepage": "https://github.com/peavers/silverstripe-full-calendar",
	"keywords": [
		"silverstripe",
		"calendar",
		"events"
	],
	"license": "BSD-3-Clause",
	"authors": [
		{
			"name": "Chris Turner",
			"email": "peavers@gmail.com"
		}
	],
	"type": "silverstripe-module",
	"extra": {
